! Rofi
rofi.font: sundae 10
rofi.padding: 10x
rofi.line-padding: 1
rofi.borderwidth: 1
rofi.color-enabled: true
rofi.hide-scrollbar: true
rofi.separator-style: solid
rofi.color-window: #1a1e21, #FF254F, #1a1e21
rofi.color-normal: #1a1e21, #f5f5f5, #1a1e21, #1a1e21, #FF254F
rofi.color-urgent: argb:00f5f5f5, #f43753, argb:00f5f5f5, argb:00f5f5f5, #FF254F
rofi.color-active: argb:00f5f5f5, #49bbfb, argb:00f5f5f5, argb:00f5f5f5, #FF254F
*.font:	sundae:pixelsize=10:antialias=true:autohint=true;
*.alpha: 0.9
! Desktop
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.dpi: 100
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xcursor.size: 8
Xft.autohint: 1

! Set the font
URxvt*cursorBlink: true
URxvt*buffered: true
URxvt.font: xft:sundae:size=10:antialias=true
URxvt.boldFont: xft:sundae:size=10
URxvt.letterSpace: 1

! Disable scrollbar
URxvt.scrollBar: false

! Set terminal padding
URxvt.internalBorder: 20

! Enables copy/paste with Ctrl-Shift-C/V
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.perl-ext-common: default,clipboard,tabbedex,confirm-paste,url-select
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.tabbedex.tabbar-fg: 4
URxvt.tabbedex.tabbar-bg: 16
URxvt.tabbedex.tab-fg:    15
URxvt.tabbedex.tab-bg:    4
URxvt.underlineURLs:     true

